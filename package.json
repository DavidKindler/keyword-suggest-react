{
  "name": "react-boilerplate-vscode",
  "version": "1.0.0",
  "description": "Boilerplate for React w/Redux and VSCode support",
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "live",
    "edit",
    "webpack",
    "babel"
  ],
  "main": "index.js",
  "repository": "git@github.com:DavidKindler/React-Boilerplate-VSCode.git",
  "scripts": {
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "watch": "webpack --watch ",
    "build": "webpack --config webpack.config.prod.js",
    "dev": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "author": "David Kindler",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "html-webpack-plugin": "^2.28.0",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "jquery": "^3.1.0",
    "mocha": "^2.4.5",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2",
    "jsdom": "^8.1.0"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "lodash": "^4.17.4",
    "react": "^15.5.0",
    "react-dom": "^16.0.1",
    "react-redux": "^5.0.0",
    "react-router": "^4.1.1",
    "redux": "^3.6.0",
    "snyk": "^1.189.0"
  },
  "snyk": true
}
